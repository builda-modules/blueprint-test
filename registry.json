{
  "name": "redfox",
  "type": "scaffold",
  "version": "1.0.0",
  "author": "Alex Foxleigh (https://www.alexfoxleigh.com/)",
  "funding": "https://www.patreon.com/builda",
  "moduleUrl": "https://github.com/builda-modules/redfox",
  "description": "This is the scaffold that I ([@foxleigh81](https://github.com/foxleigh81)) use myself to create my own projects. *Note:* You will need to install the burrow prefab (or configure storybook the same way) before you can use this scaffold as is. \n",
  "keywords": [
    "react",
    "storybook",
    "storybook-mdx",
    "typescript",
    "sass",
    "sass-modules",
    "scss",
    "scss-modules"
  ],
  "files": [
    "index.stories.mdx",
    "index.tsx",
    "styles.module.scss"
  ],
  "license": "MIT",
  "prefabs": [
    {
      "name": "the-burrows",
      "version": "^1.0.0"
    }
  ],
  "dependencies": {
    "react": "^17.0.0",
    "typescript": "^4.5.0",
    "storybook": "^6.0.0",
    "storybook-addon-essentials": "^6.0.0",
    "sass": "^1.0.0"
  },
  "substitute": [
    {
      "replace": "PREFIX",
      "required": true,
      "with": "command"
    },
    {
      "replace": "TYPE",
      "with": "component"
    }
  ]
}